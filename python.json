	"Iterator Design Pattern": {
        "prefix": "Design Pattern - Iterator",
        "body": [
            "# iterator design pattern",
            "class sumTillN():",
            "\tdef __init__(self, n):",
            "\t\tself.sum = 0",
            "\t\tself._n = n",
            "\t   ",
            "\tdef __iter__(self):",
            "\t\tself._i = 0",
            "\t\treturn self",
            "\t   ",
            "\tdef __next__(self):",
            "\t\tif(self._i < self._n):",
            "\t\t\tself.sum += self._i",
            "\t\t\tself._i+=1",
            "\t\t\treturn self.sum",
            "\t\telse:",
            "\t\t\traise StopIteration",
            "   ",
            "for i in sumTillN(10):",
            "\tprint(i) "
